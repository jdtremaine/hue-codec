{
	"name": "hue-codec",
	"version-string": "1.0.0",
	"dependencies":
	[
		"doctest",
		"fmt",
		{
			"name": "opencv",
			"features":
			[
				"jpeg",
				"png",
				"webp",
				"ffmpeg"
			],
			"$disabled_features":
			[
				"cuda"
			]
		}
	],
	"$disabled_dependencies":
	[
		"realsense2",
		{
			"name": "ffmpeg",
			"features":
			[
				"vpx",
				"x264",
				"x265"
			],
			"$disabled_features":
			[
				"nvcodec"
			]
		}
	]
}
